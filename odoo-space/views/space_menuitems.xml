<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="space_list_action" model="ir.actions.act_window">
            <field name="name">Space</field>
            <field name="res_model">space.odoo</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type='html'>
                <p class="o_view_nocontent_smiling_face">
                    Create first model space
                </p>
            </field>
        </record>
        <menuitem
            id="menu_space"
            name="Space"
            sequence="1"/>
        
        <menuitem
            id="menu_space_ sub"
            name="Space"
             parent="menu_space"
            sequence="2"/>

        <menuitem
            id="menu_space_model"
            name="Space"
            action="space_list_action"
            parent="menu_space_sub"
            sequence="3"/>
    </data>
</odoo>
